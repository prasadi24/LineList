@using LineList.Cenovus.Com.Domain.DataTransferObjects
@model HomeViewModel
@{
    Layout = "~/Views/Shared/_LayoutHome.cshtml";
    ViewData["HomeData"] = Model;
}

<div class="mt-2">
    <div class="row>
        <div class="container mt-8">
            <div class="row justify-content-center">
                <div class="col-lg-12 col-md-12">
                    <div class="card shadow-sm border-0">
                        <!-- Card Header -->
                        <div class="card-header d-flex align-items-center justify-content-between">
                            <h2 class="text-danger bold mb-0">Access Denied</h2>
                        </div>

                        <div class="card-body">
                            <div class="mb-3 alert alert-danger mb-0">You do not have permission to view this directory or page using the credentials that you supplied.</div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

